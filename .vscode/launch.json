{
    "version": "0.2.0",
    "configurations": [
      
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Next.js",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "port": 9229
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Debug Chrome",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true
      }
    ]
  }